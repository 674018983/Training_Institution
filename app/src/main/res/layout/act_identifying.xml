    android:layout_centerHorizontal="true"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content">
    <EditText
        android:id="@+id/et1"
        style="@style/identifying_Edittext"
        />
    <EditText
        android:id="@+id/et2"
        android:layout_marginLeft="10dp"
        style="@style/identifying_Edittext"
        />
    <EditText
        android:id="@+id/et3"
        android:layout_marginLeft="10dp"
        style="@style/identifying_Edittext"
        />
    <EditText
        android:id="@+id/et4"
        android:layout_marginLeft="10dp"
        style="@style/identifying_Edittext"
        />
</LinearLayout>
</RelativeLayout>
